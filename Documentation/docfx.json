{
  "metadata": [
    {
      "properties": {
        "TargetFramework": "net451"
      },
      "src": [
        {
          "src": "..",
          "files": [
            "MyProject/MyProject.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "Documentation/*"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "properties": {
      "TargetFramework": "net451"
    },
    "content": [
      {
        "src": ".",
        "files": [
          "api/toc.yml",
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "./",
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "./",
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "msdn/msdn.4.5.2/content/msdn.4.5.2.zip"
    ],
    "dest": "./_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "MyProject",
      "_appName": "MyProject",
      "_enableSearch": true,
      "_appFooter": "<span>Copyright Â© 2015-2017 Invenietis<br>Generated by <strong><a href=\"https://dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a></strong></span>"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./template"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "noLangKeyword": false,
    "keepFileLink": false
  }
}
